package fluid_modes;

public class Display extends JFrame {
	
	int time;
	
	public Display() {
		
	}
	
	public static void main(String[] args) {
        
        EventQueue.invokeLater(new Runnable() {
            public void run() {                
                Display display = new Display();
                display.setVisible(true);                
            }
        });
    }
}
