package fluid_modes;

/**
 * Generic Fluid Element
 *
 */
public class Element {
	private final double startX;
	private final double startY;
	
	double currentX;
	double currentY;
	
	public Element(double x, double y) {
		this.startX = x;
		this.startY = y;
	}
	
	public void resetPosition() {
		this.currentX = this.startX;
	}
}
